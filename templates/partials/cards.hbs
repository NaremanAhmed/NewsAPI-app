<div class="container pt-5">
  <div class="card-columns">
    {{#each data}}
      <div class="card">
        <img src="{{urlToImage}}" class="card-img-top" alt="..."
          onerror="this.src='./img/loading.gif';this.onerror='';">
        <div class="card-body">
          <h5 class="card-title">{{title}}</h5>
          <p class="card-text">{{description}}</p>
        </div>
      </div>
    {{/each}}
  </div>
</div>